{
	"env": {
		"browser": true,
		"node": true,
		"es6": true,
		"jest/globals": true
	},
	"extends": [
		"react-app",
		"plugin:import/react",
		"eslint:recommended"
	],
	"globals": {
		"Atomics": "readonly",
		"SharedArrayBuffer": "readonly"
	},
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": 2018,
		"sourceType": "module"
	},
	"plugins": [
		"jest"
	],
	"rules": {
	},
	"settings": {
		"react": {
			"version": "detect"
		}
	}
}
